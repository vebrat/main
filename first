#include <iostream>

int main() {
    // Объявляем переменные для хранения наших чисел
    int num1, num2;

    // Запрашиваем у пользователя два числа,
    std::cout << "Введите первое число: ";
    std::cin >> num1;
    std::cout << "Введите второе число : ";
    std::cin >> num2;

    // Вычисляем сумму всех чисел
    int sum = num1 + num2;

    // Выводим результат
    std::cout << "Сумма чисел: " << sum << std::endl;

    return 0;
}



)
